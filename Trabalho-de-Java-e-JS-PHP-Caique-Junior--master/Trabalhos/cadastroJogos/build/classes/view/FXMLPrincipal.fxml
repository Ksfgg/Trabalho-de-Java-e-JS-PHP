<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.FXMLPrincipalController">
   <children>
      <MenuBar layoutX="214.0" layoutY="14.0">
        <menus>
          <Menu mnemonicParsing="false" text="Arquivo">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Cadastrar">
            <items>
              <MenuItem fx:id="menuCad" mnemonicParsing="false" onAction="#cadastrar" text="Cadastrar um jogo" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Pesquisar">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#pesquisar" text="Jogos cadastrados" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
